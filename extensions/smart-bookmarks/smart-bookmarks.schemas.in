<gconfschemafile>
  <schemalist>

    <schema>
      <key>/schemas/apps/epiphany/extensions/smart-bookmarks/dictionarysearch/open_in_tab</key>
      <applyto>/apps/epiphany/extensions/smart-bookmarks/dictionarysearch/open_in_tab</applyto>
      <owner>epiphany</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Open search result in new tab</short>
