<?xml version="1.0"?>
<extension>
	<_name>Tab Groups</_name>
	<_description>Groups newly opened tabs</_description>
	<author>Justin Wake &lt;jwake@iinet.net.au&gt;</author>
	<url>http://www.gnome.org/projects/epiphany/extensions.html</url>
	<version>1</version>
	<gettext-domain>%GETTEXT_PACKAGE%</gettext-domain>
	<locale-directory>%LOCALE_DIR%</locale-directory>
	<loader type="shlib">
		<attribute name="library">%LIBRARY%</attribute>
	</loader>
</extension>
