<?xml version="1.0"?>
<extension>
	<_name>Actions</_name>
	<_description>Associate commands to pages, links and images</_description>
	<author>Jean-Yves Lefort &lt;jylefort@brutele.be&gt;</author>
	<url>http://www.gnome.org/projects/epiphany/extensions.html</url>
	<version>1</version>
	<gettext-domain>%GETTEXT_PACKAGE%</gettext-domain>
	<locale-directory>%LOCALE_DIR%</locale-directory>
	<loader type="shlib">
		<attribute name="library">%EXTENSION_DIR%/%LIBRARY%</attribute>
	</loader>
</extension>
