<?xml version="1.0"?>
<extension>
	<_name>Greasemonkey</_name>
	<_description>Run user scripts to modify web pages' behavior</_description>
	<author>Adam Hooper &lt;adamh@densi.com&gt;</author>
	<url>http://www.gnome.org/projects/epiphany/extensions.html</url>
	<version>1</version>
	<gettext-domain>%GETTEXT_PACKAGE%</gettext-domain>
	<locale-directory>%LOCALE_DIR%</locale-directory>
	<loader type="shlib">
		<attribute name="library">%EXTENSION_DIR%/%LIBRARY%</attribute>
	</loader>
</extension>
