<?xml version="1.0" encoding="UTF-8"?>
<extension>
	<name>Python Console</name>
	<description>Opens an interactive Python console</description>
	<author>James Henstridge &lt;james@daa.com.au&gt;</author>
	<author>Adam Hooper &lt;adamh@densi.com&gt;</author>
	<url>http://www.gnome.org/projects/epiphany/extensions.html</url>
	<version>1</version>
	<gettext-domain>epiphany-extensions-1.6</gettext-domain>
	<locale-directory>/opt/mozilla-1.7/share/locale</locale-directory>
	<loader type="python">
		<attribute name="module">console</attribute>
	</loader>
</extension>
