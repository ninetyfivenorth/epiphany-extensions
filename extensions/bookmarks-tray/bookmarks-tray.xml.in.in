<?xml version="1.0"?>
<extension>
	<_name>Bookmarks Tray</_name>
	<_description>Embed the bookmarks menu in the system tray</_description>
	<author>Christian Persch &lt;chpe@gnome.org&gt;</author>
	<url>http://www.gnome.org/projects/epiphany/extensions.html</url>
	<version>1</version>
	<gettext-domain>%GETTEXT_PACKAGE%</gettext-domain>
	<locale-directory>%LOCALE_DIR%</locale-directory>
	<loader type="shlib">
		<attribute name="library">%EXTENSION_DIR%/%LIBRARY%</attribute>
	</loader>
</extension>
